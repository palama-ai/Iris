# ===========================================
# IRIS Backend Environment Configuration
# ===========================================
# Copy this file to .env and fill in your values

# Server Configuration
PORT=3000

# Authentication Token (Required for secure socket connections)
# Generate a secure random token for production
AUTH_TOKEN=your_secure_auth_token_here

# Google Gemini API
# Get your API key from: https://aistudio.google.com/
GEMINI_API_KEY=your_gemini_api_key_here

# ElevenLabs Configuration
# Get your API key from: https://elevenlabs.io/
ELEVENLABS_API_KEY=your_elevenlabs_api_key_here
ELEVENLABS_AGENT_ID=your_agent_id_here
ELEVENLABS_VOICE_ID=your_voice_id_here

# Neon Database
# Get your connection string from: https://neon.tech/
DATABASE_URL=postgresql://user:password@host/database?sslmode=require
